{
  "expo": {
    "name": "setup-try-out",
    "slug": "setup-try-out",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "setuptryout",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.acerta-itp.setuptryout"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jordidepau.setuptryout"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/CrimsonText-Regular.ttf",
            "./assets/fonts/CrimsonText-Bold.ttf",
            "./assets/fonts/CrimsonText-Italic.ttf",
            "./assets/fonts/CrimsonText-BoldItalic.ttf",
            "./assets/fonts/BodoniModa-Regular.ttf",
            "./assets/fonts/BodoniModa-Bold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "reactCompiler": true
    },
    "extra": {
      "eas": {
        "projectId": "7c12fffa-4da1-4c7d-80fb-dbab67ba2186"
      }
    }
  }
}
