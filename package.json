{
  "name": "setup-try-out",
  "main": "index.ts",
  "version": "1.0.0",
  "scripts": {
    "android": "expo run:android",
    "ci:local": "pnpm code:lint && pnpm code:format && pnpm code:typecheck  && pnpm deps:cyclic && pnpm code:analyse",
    "code:analyse": "knip",
    "code:format": "prettier . --check",
    "code:format:fix": "prettier . --write",
    "code:lint": "eslint .",
    "code:lint:fix": "eslint . --fix",
    "code:typecheck": "tsc",
    "deps:cyclic": "madge --circular --extensions ts,tsx .",
    "deps:format": "syncpack format",
    "deps:lint": "syncpack lint",
    "deps:update": "syncpack update --types prod,dev",
    "deps:verify": "syncpack list-mismatches --types prod,dev,resolutions",
    "ios": "expo run:ios",
    "prebuild": "expo prebuild --clean",
    "start": "expo start",
    "start:clean": "expo start --clear"
  },
  "dependencies": {
    "expo": "54.0.32",
    "expo-splash-screen": "31.0.13",
    "expo-system-ui": "~6.0.9",
    "expo-updates": "^29.0.16",
    "react": "19.1.0",
    "react-native": "0.81.5"
  },
  "devDependencies": {
    "@types/react": "19.1.0",
    "eslint": "9.25.0",
    "eslint-config-expo": "10.0.0",
    "knip": "5.83.0",
    "madge": "8.0.0",
    "prettier": "3.8.1",
    "syncpack": "13.0.4",
    "typescript": "5.9.2"
  },
  "private": true
}
